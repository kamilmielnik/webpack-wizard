{
  "name": "$name",
  "version": "$version",
  "license": "$license",
  "author": {
    "name": "$authorName",
    "email": "$authorEmail"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-wizard start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "dependencies": {
    "classnames": "2.2.5",
    "normalize.css": "7.0.0",
    "prop-types": "15.6.0",
    "re-reselect": "1.0.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "better-npm-run": "0.1.0",
    "http-server": "0.10.0",
    "react-hot-loader": "3.1.3",
    "webpack-wizard": "0.1.0-beta.1"
  }
}
