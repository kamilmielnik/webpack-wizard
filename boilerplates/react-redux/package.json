{
  "name": "$name",
  "version": "$version",
  "license": "$license",
  "author": {
    "name": "$authorName",
    "email": "$authorEmail"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "analyze-bundle": "better-npm-run analyze-bundle",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "analyze-bundle": {
      "command": "webpack-wizard build",
      "env": {
        "ANALYZE_BUNDLE": true,
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack-wizard build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-wizard start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "devDependencies": {
    "better-npm-run": "0.1.0",
    "http-server": "0.11.1",
    "react-hot-loader": "4.1.2",
    "redux-devtools-extension": "2.13.2",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-wizard": "0.1.0-beta.25"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "node-normalize-scss": "^3.0.0",
    "prop-types": "15.6.1",
    "re-reselect": "2.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-actions": "2.3.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1"
  }
}
